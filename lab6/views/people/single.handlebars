<article class="people">
	<div class="well well-sm blockquote">
	<h3>Name: {{person.name}}</h3>
	<h4>ID: {{person.id}}</h4>
	</div>
	<div class="well well-sm blockquote">
	{{#each eventList}}
	<ul>
		<li><strong>Event Title: </strong><a href="../events/{{id}}">{{title}}</a></li>
		<li><strong>Event ID: </strong>{{id}}</li>
		<li><strong>Attendees' ID: </strong>{{attendees}}</li>
		<li><strong>Start Time: </strong>{{startTime}}</li>
		<li><strong>Description: </strong>{{description}}</li>
		<li><strong>Location: </strong>{{location}}</li>
	</ul>
	{{/each}}
	</div>
</article>