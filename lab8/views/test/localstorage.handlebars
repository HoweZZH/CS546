    <thead>
    <tr>
        <!--<th>Property Name</th>-->
        <th>Key Name</th>
        <!--<th>Value</th>-->
        <th>Key Value</th>
        <!--<th>Type After JSON Parse</th>-->
    </tr>
    </thead>
    <tbody>
    </tbody>
</table>
<form class="form form-horizontal" id="localstorage-form">
    <div class="form-group">
        <label for="localstorage-value" class="col-sm-2 control-label">Value</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="localstorage-value" placeholder="input value">
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <button type="submit" class="btn btn-default">Submit Form</button>
            <button type="button" class="btn btn-danger"  style="color: #ffffff; background-color: #d4423c;" id="clear-storage">reset LocalStorage</button>
        </div>
    </div>
</form>
<div class="alert alert-danger hidden" style="color: #ffffff; background-color: #d4423c;" id="form-alert">
</div>